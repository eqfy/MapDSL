{
  "name": "cpsc-410-dsl-project",
  "description": "A dsl for map generation",
  "repository": "https://github.students.cs.ubc.ca/CPSC410-2022W-T1/Project1Group9",
  "scripts": {
    "build": "concurrently \"npm:buildClient\" \"npm:buildServer\"",
    "buildClient": "mkdir -p public && cd ./src/client/ && npm run build",
    "buildServer": "mkdir -p build && cd ./src/server/ && npm run build",
    "start": "npm run build && concurrently \"npm:startClient\" \"npm:startServer\"",
    "startClient": "cd ./src/client/ && npm run start",
    "startServer": "cd ./src/server/ && npm run start",
    "buildWin": "concurrently \"npm:buildClientWin\" \"npm:buildServerWin\"",
    "buildClientWin": "if not exist public mkdir public && cd ./src/client/ && npm run build",
    "buildServerWin": "if not exist build mkdir build && cd ./src/server/ && npm run build",
    "startWin": "npm run buildWin && concurrently \"npm:startClient\" \"npm:startServer\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/google.maps": "^3.50.2",
    "@types/node": "^18.7.20",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "concurrently": "^7.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.24.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.14.3",
    "antlr4ts": "^0.5.0-alpha.4",
    "cors": "^2.8.5",
    "express": "^4.18.1"
  }
}
